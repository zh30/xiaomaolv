# MiniMax (CN endpoint): https://api.minimaxi.com/v1
MINIMAX_API_KEY=replace_with_your_minimax_coding_plan_key
# Optional: override model, default is MiniMax-M2.5-highspeed
MINIMAX_MODEL=MiniMax-M2.5-highspeed

# Telegram Bot token from @BotFather
TELEGRAM_BOT_TOKEN=replace_with_your_telegram_bot_token
# Optional: bot username (without '@'), improves group @mention matching startup behavior
TELEGRAM_BOT_USERNAME=replace_with_your_bot_username
# Optional: bot profile startup status text (setMyShortDescription)
TELEGRAM_STARTUP_ONLINE_TEXT=online
# Optional: group trigger mode (strict | smart), default recommended is smart
TELEGRAM_GROUP_TRIGGER_MODE=smart
# Optional: scheduler default timezone (IANA), e.g. Asia/Shanghai / America/Los_Angeles
TELEGRAM_SCHEDULER_DEFAULT_TIMEZONE=Asia/Shanghai
# Optional: admin user ids for private chat access and /mcp command (comma-separated, no spaces needed)
# Example: TELEGRAM_ADMIN_USER_IDS=123456789,987654321
TELEGRAM_ADMIN_USER_IDS=

# Optional: only required when memory.backend = "hybrid-sqlite-zvec"
ZVEC_SIDECAR_ENDPOINT=http://127.0.0.1:3711
ZVEC_SIDECAR_TOKEN=replace_with_sidecar_bearer_token
